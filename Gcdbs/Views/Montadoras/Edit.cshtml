@model Gcdbs.Models.Montadora

@{
    ViewData["Title"] = "Editar Montadora";
}

<h2>Editar Montadora</h2>

<h4>Montadora</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="IdMontadora" />
            <div class="form-group">
                <label asp-for="NomeMontadora" class="control-label"></label>
                <input asp-for="NomeMontadora" class="form-control" />
                <span asp-validation-for="NomeMontadora" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Voltar</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
